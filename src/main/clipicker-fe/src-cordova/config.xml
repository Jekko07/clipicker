<?xml version='1.0' encoding='utf-8'?>
<widget id="com.clipicker.me" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CliPicker</name>
    <description>CliPicker for you</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <allow-navigation href="https://clipicker.club/*" />
    <allow-navigation href="https://dev-553248.oktapreview.com/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1" />
    <icon src="res/clipicker.png" />
    <plugin name="org.apache.cordova.geolocation" spec="^0.3.6" />
    <plugin name="cordova-plugin-local-notification" spec="^0.9.0-beta.2" />
    <plugin name="cordova-plugin-calendar" spec="^5.1.2" />
    <preference name="android-build-tool" value="gradle" />
    <plugin name="im.ltdev.cordova.UserAgent" spec="https://github.com/LouisT/cordova-useragent" />
    <engine name="android" spec="^7.1.0" />
</widget>
